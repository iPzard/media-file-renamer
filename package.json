{
  "author": "Daniel Wade",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fluentui/react": "^7.121.12",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "get-port": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5"
  },
  "description": "Fix names of tv show episode files",
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-packager": "^15.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "jsdoc": "^3.6.5",
    "prettier": "^2.0.4",
    "sass": "^1.26.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "./",
  "license": "MIT",
  "main": "main.js",
  "name": "media-name-fixer",
  "private": true,
  "productName": "Media File Renamer",
  "scripts": {
    "build": "react-scripts build",
    "build:docs": "jsdoc -c jsdoc.json",
    "build:all": "electron-packager . media-file-renamer --asar --all --icon ./public/favicon.ico --out ./dist --overwrite",
    "build:linux": "electron-packager . media-file-renamer --asar --icon ./public/favicon.ico  --linux --out ./dist --overwrite",
    "build:mac": "electron-packager . media-file-renamer --asar --icon ./public/favicon.ico --darwin --out ./dist --overwrite",
    "build:windows": "electron-packager . media-file-renamer --asar --icon ./public/favicon.ico --win32 --out ./dist --overwrite",
    "eject": "react-scripts eject",
    "start": "electron .",
    "test": "react-scripts test"
  },
  "version": "1.0.0"
}
